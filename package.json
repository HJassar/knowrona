    "url": "https://github.com/cnsplit/knowrona/issues"
  },
  "homepage": "https://github.com/cnsplit/knowrona#readme",
  "dependencies": {
    "async": "^3.2.0",
    "axios": "^0.20.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-sslify": "^1.2.0",
    "flash": "^1.1.0",
    "is-empty": "^1.2.0",
